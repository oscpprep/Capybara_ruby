# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore .env file containing sensitive information.
.env

# Ignore the system's temporary files.
*.swp
*~

# Ignore coverage directory generated by tools like SimpleCov.
/coverage/

# Ignore Gemfile.lock, as it's generally recommended to not include it in version control for Ruby projects.
Gemfile.lock

# Ignore the default VSCode settings directory.
.vscode/

# Ignore RVM config files.
.rvmrc
.ruby-version
